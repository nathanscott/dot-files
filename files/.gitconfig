	email = nathan@inspire9.com
[color]
	diff = auto
	status = auto
	branch = auto
[core]
	excludesfile = /Users/nathan/.gitignore
[github]
	user = nathanscott
[branch "master"]
  remote = origin
  merge = master
[branch]
  autosetupmerge = true 
[mergetool]
	keepBackup = true
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[push]
	default = simple
